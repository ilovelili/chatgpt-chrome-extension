{
  "manifest_version": 3,
  "name": "ChatExt",
  "version": "1.0",
  "permissions": [
    "tabs"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [{
    "matches": ["https://chat.openai.com/*"],
    "js": ["content.js"]
  }]
}